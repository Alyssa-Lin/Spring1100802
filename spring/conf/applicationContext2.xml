<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- 設定檔 -->
	<bean id="student1" class="com.study.spring.case02.Student"
		p:id="10662" p:name="YIKO" init-method="init" destroy-method="destroy" >

		<property name="clazzs">
			<set>
				<ref bean="clazz1"/>
				<ref bean="clazz3"/>
				<ref bean="clazz2"/>
				</set>
		</property>
		


		
		</bean>


	<bean id="clazz1" class="com.study.spring.case02.Clazz"
		p:id="101" p:name="english" p:credit="6" />
	<bean id="clazz2" class="com.study.spring.case02.Clazz"
		p:id="306" p:name="chinese" p:credit="8" />
	<bean id="clazz3" class="com.study.spring.case02.Clazz"
		p:id="809" p:name="Math" p:credit="15" />


</beans>
